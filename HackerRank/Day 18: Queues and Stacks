public class Solution {
    // Write your code here.
    Queue<Character> q = new LinkedList<>();
    Stack<Character> st = new Stack<>();
    
    void pushCharacter(char ch)
    {
        st.push(ch);
    }
    void enqueueCharacter(char ch)
    {
        q.add(ch);
    }
    
    char popCharacter()
    {
        return(st.pop());
    }
    
    char dequeueCharacter()
    {
        return(q.remove());
    }
    }
