public class Main
{
    public static void Sum(TreeNode root)
    {
        if(root==null)return;
        Sum(root.left);
        Sum(root.right);
        if(root.left!=null)root.val = root.val + root.left.val;
        if(root.right!=null)root.val = root.val + root.right.val;
    }
    
    
	public static void main(String[] args) {
		Sum(treenode1);
	}
}
