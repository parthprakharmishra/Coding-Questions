
import java.io.*;
import java.util.*;

interface DetermineTotal
{
    public void calcTotal();
}

class QuizMarks
{
    String StudentName;

    QuizMarks()
    {
        StudentName="";
    }

    QuizMarks(String s)
    {
        StudentName=s;
    }
}

class Result extends QuizMarks
{

    int QuestionCount, CorrectCount, WrongCount, TotalMarks;
    Result()
    {
        QuestionCount=0;
        CorrectCount=0;
        WrongCount=0;
        TotalMarks=0;
    }

    Result(int qc, int cc, int wc)
    {
        QuestionCount=qc;
        CorrectCount=cc;
        WrongCount=wc;
    
    }
    void display(int t)
    {
        System.out.println("Total Marks = "+t);
    }
}

class marks extends Result implements DetermineTotal
{
    static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));

    void input()
    {
        System.out.println("Enter the name");
        String StudentName = br.readLine();

        System.out.println("Enter number of total questions");
        int QuestionCount = Integer.parseInt(br.readLine());

        System.out.println("Enter number of correct answers");
        int CorrectCount = Integer.parseInt(br.readLine());

        System.out.println("Enter number of wrong answers");
        int WrongCount = Integer.parseInt(br.readLine());

    }

    public void calcTotal()
    {
        TotalMarks = 2*CorrectCount-WrongCount;
        Result ob = new Result(QuestionCount, CorrectCount, WrongCount);
        ob.display(TotalMarks);
    }
}

class Q2_19BIT0390
{
    static BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
    
    public static void main(String args[])throws IOException
    {
        
        System.out.println("Enter number of objects");
        int n = Integer.parseInt(br.readLine());

        

       

        marks obj[] = new marks[n];
        List<Result> list = new ArrayList<>();
        for(int i=0;i<n;++i)
        {
            obj[i].calcTotal();
        }
    }
}




