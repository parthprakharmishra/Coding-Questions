class Solution {
    public int maximumUniqueSubarray(int[] nums) {
        
        int max=Integer.MIN_VALUE,sum=0;
        HashSet hs = new HashSet();
        for(int i=0, j=0;i<nums.length;++i)
        {
            while(!hs.add(nums[i]))
            {
                sum-=nums[j];
                hs.remove(nums[j++]);
            }
            sum+=nums[i];
            max=Math.max(max,sum);
        }
        return(max);
    }
}
