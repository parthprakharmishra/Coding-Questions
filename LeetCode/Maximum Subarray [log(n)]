class Solution {
    public int maxSubArray(int[] nums) 
    {
        int s=0;
        int max=Integer.MIN_VALUE;  
        for(int i=0;i<nums.length;++i)
        {
            s=s+nums[i];
            if(max<=s)
            {
                max=Math.max(s,max);
            }
            if(nums[i]>s)
            {
                s=nums[i];
                max=Math.max(s,max);
            }
            
        }
        return(max);
    }
}
