class Solution {
    public List<String> buildArray(int[] target, int n) {
        
        ArrayList<String> str = new ArrayList<>();
        
        int k=0;
        for(int i=0;i<n;++i)
        {
            if(target.length>k)
            {
                str.add("Push");
                if(target[k]!=i+1)str.add("Pop");
                else ++k;
            }
            
        }
        return(str);
        
    }
}
