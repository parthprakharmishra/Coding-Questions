class Solution {
    public int maxScore(int[] cardPoints, int k) {
        int sum=0, csum=0;
        for(int i=0;i<k;++i){
            sum+=cardPoints[i];
        }
        
        csum=sum;
        int g=cardPoints.length-1;
        for(int j=k-1;j>=0;--j){
            csum-=cardPoints[j];
            csum+=cardPoints[g];
            --g;
            if(csum>sum)sum=csum;
        }
        return(sum);
    }
}
