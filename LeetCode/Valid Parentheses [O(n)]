class Solution {
    public boolean isValid(String s) 
    {
        Stack<Character> st = new Stack<>();
        
        int g=0;
        for(int i=0;i<s.length();++i)
        {
            char ch = s.charAt(i);
            if(ch=='(' || ch=='[' || ch=='{')st.push(ch);
            else if(ch==')')
            {
                if(st.isEmpty()==false && st.peek()=='(')
                {
                    st.pop();
                }
                else
                {
                    g=-1;
                    break;
                }
            }
            else if(ch==']')
            {
                if(st.isEmpty()==false && st.peek()=='[')
                {
                    st.pop();
                }
                else
                {
                    g=-1;
                    break;
                }
            }
            else if(ch=='}')
            {
                if(st.isEmpty()==false && st.peek()=='{')
                {
                    st.pop();
                }
                else
                {
                    g=-1;
                    break;
                }
            }
            else;
        }
        
        if(g==0 && st.size()==0)return(true);
        else return(false);
    }
}
